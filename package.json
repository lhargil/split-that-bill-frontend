{
  "name": "splitthatbill",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=dev -o",
    "start:local": "ng serve -o --configuration=local",
    "start:prod": "ng serve -o --prod",
    "build": "ng build",
    "build:local": "ng build --configuration=local",
    "build:dev": "ng build --configuration=dev",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "coverage": "ng test --code-coverage --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run splitthatbill:serve-ssr",
    "serve:ssr": "node dist/splitthatbill/server/main.js",
    "build:ssr": "ng build --prod && ng run splitthatbill:server:production",
    "prerender": "ng run splitthatbill:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.5",
    "@angular/common": "^9.0.5",
    "@angular/compiler": "^9.0.5",
    "@angular/core": "^9.0.5",
    "@angular/forms": "^9.0.5",
    "@angular/platform-browser": "^9.0.5",
    "@angular/platform-browser-dynamic": "^9.0.5",
    "@angular/platform-server": "^9.0.5",
    "@angular/router": "^9.0.5",
    "@auth0/auth0-spa-js": "^1.6.4",
    "@nguniversal/express-engine": "^9.0.1",
    "express": "^4.17.1",
    "luxon": "^1.22.0",
    "postcss-import": "^12.0.1",
    "rxjs": "~6.5.4",
    "tailwindcss": "^1.2.0",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.0.0",
    "@angular-devkit/build-angular": "~0.900.5",
    "@angular/cli": "^9.0.5",
    "@angular/compiler-cli": "^9.0.5",
    "@angular/language-service": "^9.0.5",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@nguniversal/builders": "^9.0.1",
    "@types/express": "^4.17.3",
    "@types/jasmine": "~3.5.8",
    "@types/jasminewd2": "~2.0.8",
    "@types/luxon": "^1.21.0",
    "@types/node": "^13.9.0",
    "codelyzer": "^5.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "postcss-scss": "^2.0.0",
    "protractor": "^5.4.3",
    "ts-node": "~8.6.2",
    "tslint": "~6.0.0",
    "typescript": "~3.7.5"
  }
